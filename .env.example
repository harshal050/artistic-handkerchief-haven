
# MongoDB Connection
# Option 1: MongoDB Atlas URI
# VITE_MONGODB_URI=mongodb+srv://your_username:your_password@your_cluster.mongodb.net/your_database?retryWrites=true&w=majority

# Option 2: Local MongoDB (make sure MongoDB is running locally)
VITE_MONGODB_URI=mongodb://localhost:27017/gwokdatabase

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret
CLOUDINARY_UPLOAD_PRESET=your_upload_preset  # For browser uploads, create an unsigned upload preset in Cloudinary
